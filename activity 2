import turtle
turtle.Screen().bgcolor("aqua")
hi=turtle.Turtle()
hi.forward(100)
hi.left(120)
hi.forward(100)
hi.left(120)
hi.forward(100)
hi.up()
hi.right(150)
hi.forward(50)
hi.down()
hi.right(90)
hi.forward(100)
hi.right(120)
hi.forward(100)
hi.right(120)
hi.forward(100)
turtle.done()